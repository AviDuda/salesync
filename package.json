{
  "name": "sale-events-tool",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs --target=es2021 ./server.ts --outdir=build --bundle",
    "db:format": "prisma format",
    "db:dev:setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "db:dev:reset": "prisma db push && prisma db seed",
    "db:dev:reset:force": "prisma db push --force-reset && prisma db seed",
    "db:studio": "prisma studio",
    "dev": "run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development pnpm run build:server --watch",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/server.js",
    "docker": "docker-compose up -d",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "postinstall": "./postinstall.sh",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"pnpm exec cypress open\"",
    "test:e2e:run": "pnpm run build && cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"pnpm exec cypress run\"",
    "typecheck": "tsc && tsc -p cypress",
    "typecheck:remix:watch": "tsc --watch --noEmit",
    "typecheck:cypress:watch": "tsc --watch --noEmit -p cypress",
    "validate": "run-p \"test --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "dependencies": {
    "@conform-to/dom": "^0.6.3",
    "@conform-to/react": "^0.6.3",
    "@conform-to/zod": "^0.6.3",
    "@prisma/client": "^4.15.0",
    "@remix-run/css-bundle": "^1.17.0",
    "@remix-run/express": "^1.17.0",
    "@remix-run/node": "^1.17.0",
    "@remix-run/react": "^1.17.0",
    "@remix-run/server-runtime": "^1.17.0",
    "@remix-validated-form/with-zod": "^2.0.5",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "isbot": "^3.6.10",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-typedjson": "^0.1.7",
    "remix-utils": "^6.4.0",
    "remix-validated-form": "^4.6.12",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.21.4",
    "zod-form-data": "^2.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@remix-run/dev": "^1.17.0",
    "@remix-run/eslint-config": "^1.17.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie": "^0.5.1",
    "@types/eslint": "^8.40.1",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.6",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/parser": "^5.59.9",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^0.32.0",
    "autoprefixer": "^10.4.14",
    "c8": "^7.14.0",
    "cookie": "^0.5.0",
    "cypress": "^12.14.0",
    "dotenv": "^16.1.4",
    "esbuild": "^0.18.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-json-files": "^2.2.0",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-unicorn": "^47.0.0",
    "happy-dom": "^9.20.3",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.15.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^3.11.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.0"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "pnpm@8.6.1",
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
